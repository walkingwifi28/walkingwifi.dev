{
    "mcpServers": {
        "textlint": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "textlint",
                "--mcp"
            ],
            "env": {}
        },
        "context7": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
            "env": {}
        }
    }
}